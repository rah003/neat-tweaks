<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>neat-tweaks-developers</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <class>com.neatresults.mgnltweaks.NeatTweaks4DevelopersModule</class>
  <versionHandler>com.neatresults.mgnltweaks.setup.NeatTweaks4DevelopersVersionHandler</versionHandler>
  <version>${project.version}</version>

  <components>
    <id>subapp</id>
    <component>
      <type>info.magnolia.ui.api.availability.AvailabilityChecker</type>
      <implementation>com.neatresults.mgnltweaks.action.availability.RootAndPropAppAvailabilityCheckerImpl</implementation>
    </component>
  </components>
  <components>
    <id>app-neatconfiguration-status</id>
    <component>
      <type>com.neatresults.mgnltweaks.app.status.ConfigStatusSubApp</type>
      <implementation>com.neatresults.mgnltweaks.app.status.ConfigStatusSubApp</implementation>
    </component>
    <component>
      <type>com.neatresults.mgnltweaks.app.status.ConfigStatusView</type>
      <implementation>com.neatresults.mgnltweaks.app.status.ConfigStatusViewImpl</implementation>
    </component>
    <component>
      <type>com.neatresults.mgnltweaks.app.status.ConfigStatusPresenter</type>
      <implementation>com.neatresults.mgnltweaks.app.status.ConfigStatusPresenter</implementation>
    </component>
  </components>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.3.5/*</version>
    </dependency>
  <!-- Add other dependencies here, e.g the standard-templating-kit, or the multisite module with the enterprise edition. -->
    <dependency>
      <name>magnolia-imaging</name>
      <version>1.0/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>standard-templating-kit</name>
      <version>2.7/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>inplace-templating</name>
      <version>1.0/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>resources</name>
      <version>1.0/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>pages</name>
      <version>1.0/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>multisite</name>
      <version>1.0/*</version>
      <optional>true</optional>
    </dependency>
  </dependencies>
</module>
